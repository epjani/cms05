<%=link_to "New User", new_admin_user_path(), :class => 'btn' %>
<br/><br/>

<table class="table table-condensed table-bordered">
  <tr>
  	<td>Id</td>
    <td>Email</td>
    <td>Actions</td>
  </tr>	
  <% @users.each do |user| %>
  	<tr>
  		<td><%= user.id %></td>
  		<td><%= user.email %></td>
  		<td>
  			<%= link_to 'Edit', edit_admin_user_url(user.id)%> | 
  			<%= link_to 'Delete', admin_user_path(user.id), :method => :delete, :class => :destroy %>
  		</td>
  	</tr>
  <% end %>
</table>